		<xsd:annotation>
			<xsd:documentation>An xlink attribute group for defining the end point of a relationship.</xsd:documentation>
			<xsd:documentation>term: Resource Endpoint</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute ref="xlink:label"/>
	</xsd:attributeGroup>
